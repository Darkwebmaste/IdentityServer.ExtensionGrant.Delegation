<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <Company>www.ebenmonney.com</Company>
    <PackageTags>IdentityServer4 ExtensionGrant Delegation TokenExchange Google Facebook Twitter Social Login QuickApp</PackageTags>
    <RepositoryType>Git</RepositoryType>
    <PackageProjectUrl>https://ebenmonney.com</PackageProjectUrl>
    <Authors>Ebenezer Monney</Authors>
    <Product>QuickApp</Product>
    <Version>1.0.0-alpha</Version>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/emonney/IdentityServer.ExtensionGrant.Delegation.git</RepositoryUrl>
    <Copyright>www.ebenmonney.com</Copyright>
